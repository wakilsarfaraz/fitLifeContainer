{
  "name": "Django + Djongo Dev Container",
  "dockerFile": "Dockerfile",
  "appPort": [8000],
  "postCreateCommand": "mongod --fork --logpath /var/log/mongodb.log --dbpath /data/db",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python", "ms-python.vscode-pylance"]
    }
  },
  "remoteUser": "root"
}
